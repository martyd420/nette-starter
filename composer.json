{
	"name": "martyd420/nette-starter",
	"description": "Nette Starter",
	"keywords": ["nette"],
	"type": "project",
	"license": ["MIT", "BSD-3-Clause", "GPL-2.0-only", "GPL-3.0-only"],
	"require": {
		"php": ">= 8.2",
		"nette/application": "^3.2.3",
		"nette/assets": "^1.0.0",
		"nette/bootstrap": "^3.2.6",
		"nette/caching": "^3.2",
		"nette/database": "^3.2",
		"nette/di": "^3.2",
		"nette/forms": "^3.2",
		"nette/http": "^3.3",
		"nette/mail": "^4.0",
		"nette/robot-loader": "^4.0",
		"nette/security": "^3.2",
		"nette/utils": "^4.1",
		"latte/latte": "^3.1",
		"tracy/tracy": "^2.10",
		"nettrine/dbal": "^0.10.3",
		"nettrine/orm": "^0.10.1",
		"nettrine/extra": "^0.2.0",
		"nettrine/fixtures": "^0.9.0",
		"nettrine/migrations": "^0.10.1",
		"contributte/console": "^0.11.0",
		"contributte/event-dispatcher": "^0.9.1",
		"ublaboo/datagrid": "^7.1",
		"contributte/translation": "^2.0",
		"orisai/tracy-pets": "^1.0"
	},
	"require-dev": {
		"nette/tester": "^2.5",
		"phpstan/phpstan-nette": "^2.0",
		"symfony/thanks": "^1",
		"fakerphp/faker": "^1.24",
		"phpstan/phpstan-doctrine": "^2.0",
		"friendsofphp/php-cs-fixer": "^3.92"
	},
	"autoload": {
		"psr-4": {
			"App\\": "private/app"
		}
	},
	"scripts": {
		"phpstan": "private/vendor/bin/phpstan analyse private/app",
		"cs:check": "private/vendor/bin/php-cs-fixer fix --dry-run --diff",
		"cs:fix": "private/vendor/bin/php-cs-fixer fix",
		"tester": "private/vendor/bin/tester tests -s",
		"db:reset": [
			"php private/cli/console.php orm:schema-tool:drop --force",
			"rm -rf private/temp/cache/*",
			"php private/cli/console.php orm:schema-tool:create",
			"php private/cli/console.php doctrine:fixtures:load --no-interaction"
		]
	},
	"minimum-stability": "stable",
	"config": {
		"vendor-dir": "private/vendor",
		"allow-plugins": {
			"symfony/thanks": true
		}
	}
}
